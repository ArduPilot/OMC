<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
    <Document>
        <name>Discrete Level Of Detail</name>

        <!-- clampToGround -->
        <Folder>
            <name>clampToGround</name>
            <Placemark>
                <name>First Level (clampToGround)</name>
                <Region>
                    <LatLonAltBox>
                        <north>40</north>
                        <south>30</south>
                        <east>-115</east>
                        <west>-125</west>
                    </LatLonAltBox>
                    <Lod>
                        <minLodPixels>0</minLodPixels>
                        <maxLodPixels>128</maxLodPixels>
                    </Lod>
                </Region>
                <Style>
                    <LineStyle>
                        <color>80ffffff</color>
                    </LineStyle>
                    <PolyStyle>
                        <color>80ff0000</color>
                    </PolyStyle>
                </Style>
                <Polygon>
                    <altitudeMode>clampToGround</altitudeMode>
                    <outerBoundaryIs>
                        <LinearRing>
                            <coordinates>
                                -125,30,1e5 -115,30,1e5 -115,40,1e5 -125,40,1e5 -125,30,1e5
                            </coordinates>
                        </LinearRing>
                    </outerBoundaryIs>
                </Polygon>
            </Placemark>
            <Placemark>
                <name>Second Level (clampToGround)</name>
                <Region>
                    <LatLonAltBox>
                        <north>40</north>
                        <south>30</south>
                        <east>-115</east>
                        <west>-125</west>
                    </LatLonAltBox>
                    <Lod>
                        <minLodPixels>128</minLodPixels>
                        <maxLodPixels>512</maxLodPixels>
                    </Lod>
                </Region>
                <Style>
                    <LineStyle>
                        <color>80ffffff</color>
                    </LineStyle>
                    <PolyStyle>
                        <color>8000ff00</color>
                    </PolyStyle>
                </Style>
                <Polygon>
                    <altitudeMode>clampToGround</altitudeMode>
                    <outerBoundaryIs>
                        <LinearRing>
                            <coordinates>
                                -125,30,1e5 -115,30,1e5 -115,40,1e5 -125,40,1e5 -125,30,1e5
                            </coordinates>
                        </LinearRing>
                    </outerBoundaryIs>
                </Polygon>
            </Placemark>
            <Placemark>
                <name>Third Level (relativeToGround)</name>
