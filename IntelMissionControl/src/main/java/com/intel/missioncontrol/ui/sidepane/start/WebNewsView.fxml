<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootNode" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.intel.missioncontrol.ui.sidepane.start.WebNewsView"
            managed="false" visible="false" >

    <!--
        This FXML file will be loaded on the JavaFX launcher thread, but a WebView can only be
        created on the JavaFX application thread. We will create and insert the WebView later.
     -->

    <Button fx:id="hideButton" styleClass="large-transparent-icon-button,icon-close-inverse"
            AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <tooltip>
            <Tooltip text="%com.intel.missioncontrol.ui.sidepane.start.WebNewsView.close"/>
        </tooltip>
    </Button>

</AnchorPane>
