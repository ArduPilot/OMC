<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>

<TitledPane collapsible="false" contentDisplay="RIGHT" stylesheets="@style.css" text="Control" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.intel.dronekitsample.ui.ControlView">
    <HBox fx:id="root" alignment="CENTER_LEFT" fillHeight="false" spacing="5.0">
        <ChoiceBox fx:id="modeChoiceBox" prefWidth="150.0">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin></ChoiceBox>
        <Button fx:id="armButton" minWidth="65.0" mnemonicParsing="false" text="Arm" />
        <Button fx:id="launchButton" minWidth="65.0" mnemonicParsing="false" text="Takeoff" />
      <Button fx:id="altButton" mnemonicParsing="false" text="Alt..." />
    </HBox>
   <graphic>
      <Label fx:id="stateLabel" alignment="BASELINE_RIGHT" styleClass="tiled-pane-annotation" text="Label">
      </Label>
   </graphic>

</TitledPane>
